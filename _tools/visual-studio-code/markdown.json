{
    "Comment Afkortingen": {
        "prefix": "afkortingen",
        "body": [
            "{% comment %}",
            "<!-- ⚓ Afkortingen -->",
            "{% endcomment %}",
            "*[]:                        ?"
        ],
        "description": "Comment Afkortingen"
    },
