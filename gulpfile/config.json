{
  "destinations": [
    "artestead-doc",
    "cmp",
    "dotfiles-doc",
    "ed",
    "mma",
    "nmdad2",
    "nmdad3",
    "webd"
  ],
  "dir": {
    "node": "./node_modules/",
    "src": "./src/"
  },
  "sass": {
